{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "quotes": [
      "warn",
      "double"
    ],
    "jsx-quotes": [
      "warn",
      "prefer-double"
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always",
        "exports": "always-multiline",
        "functions": "always-multiline"
      }
    ]
  }
}